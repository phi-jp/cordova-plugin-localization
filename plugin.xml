<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-localization" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
  <name>Localization Plugin</name>
  
  <platform name="ios">
    <config-file target="*-Info.plist" parent="CFBundleDevelopmentRegion">
      <array>
        <string>English</string>
        <string>Japanese</string>
      </array>
    </config-file>
    <config-file target="*-Info.plist" parent="CFBundleLocalizations">
      <array>
        <string>en</string>
        <string>ja</string>
      </array>
    </config-file>
  </platform>
</plugin>